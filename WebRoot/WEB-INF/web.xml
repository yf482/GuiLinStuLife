<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.glife.front.servlet.LoginServlet</servlet-class>
    <init-param>
    	<param-name>encoding</param-name>
    	<param-value>UTF-8</param-value>
    </init-param>
    <init-param>
    	<param-name>cfg</param-name>
    	<param-value>yunfeng</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>ValidationCode</servlet-name>
    <servlet-class>com.glife.front.servlet.ValidationCode</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletDemo1</servlet-name>
    <servlet-class>com.glife.front.servlet.ServletDemo1</servlet-class>
    <init-param>
    	<param-name>encoding</param-name>
    	<param-value>UTF-8</param-value>
    </init-param>
    <init-param>
    	<param-name>cfg</param-name>
    	<param-value>yunfeng</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>ImgServlet</servlet-name>
    <servlet-class>com.glife.front.servlet.ImgServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DispatchServletDemo</servlet-name>
    <servlet-class>com.glife.front.servlet.DispatchServletDemo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InitServletDemo</servlet-name>
    <servlet-class>com.glife.web.servlet.test.InitServletDemo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RequestServlet</servlet-name>
    <servlet-class>com.glife.web.servlet.test.RequestServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CodeServlet</servlet-name>
    <servlet-class>com.glife.web.servlet.test.CodeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ForwardServlet1</servlet-name>
    <servlet-class>com.glife.web.servlet.test.ForwardServlet1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ForwardServlet2</servlet-name>
    <servlet-class>com.glife.web.servlet.test.ForwardServlet2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>IncludeServlet</servlet-name>
    <servlet-class>com.glife.web.servlet.test.IncludeServlet</servlet-class>
  </servlet>









 


  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ValidationCode</servlet-name>
    <url-pattern>/servlet/ValidationCode</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletDemo1</servlet-name>
    <url-pattern>/servlet/ServletDemo1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ImgServlet</servlet-name>
    <url-pattern>/servlet/ImgServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DispatchServletDemo</servlet-name>
    <url-pattern>/servlet/DispatchServletDemo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InitServletDemo</servlet-name>
    <url-pattern>/servlet/InitServletDemo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RequestServlet</servlet-name>
    <url-pattern>/servlet/RequestServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CodeServlet</servlet-name>
    <url-pattern>/servlet/CodeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ForwardServlet1</servlet-name>
    <url-pattern>/servlet/ForwardServlet1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ForwardServlet2</servlet-name>
    <url-pattern>/servlet/ForwardServlet2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IncludeServlet</servlet-name>
    <url-pattern>/servlet/IncludeServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
